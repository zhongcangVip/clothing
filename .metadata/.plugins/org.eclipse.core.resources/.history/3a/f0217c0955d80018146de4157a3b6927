<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.clothing.dao.item.ItemSupplierDao">
	<resultMap id="ItemSupplierResult" type="com.clothing.model.item.ItemSupplier">
		<result property="id" column="fid" />
		<result property="cu" column="fcu" />
		<result property="code" column="fcode" />
		<result property="name" column="fname" />
		<result property="address" column="faddress" />
		<result property="contacts" column="fcontacts" />
		<result property="mobile" column="fmobile" />
		<result property="qq" column="fqq" />
		<result property="wx" column="fwx" />
		<result property="email" column="femail" />
		<result property="status" column="fstatus" />
		<result property="userId" column="fuserid" />
		<result property="createTime" column="fcreatetime" />
		<result property="lastupdateUserId" column="flastupdateuserid" />
		<result property="lastupdateTime" column="flastupdatetime" />
	</resultMap>
	<select id="getItemSupplierById" parameterType="string"
		resultMap="ItemSupplierResult">
		SELECT T.*
		FROM t_clothing_itemsupplier T where T.fid=#{id}
	</select>
	<select id="getItemSupplierByList" parameterType="map"
		resultMap="ItemSupplierResult">
		SELECT data.*
		FROM t_clothing_itemsupplier data
		<where> 1=1
		</where>
	</select>
	<insert id="insertItemSupplier" parameterType="com.clothing.model.item.ItemSupplier">
		insert into
		t_clothing_itemsupplier(fid,fcode,fname,faddress,fcontacts,fmobile,fqq,fwx,femail,fstatus,fuserid,fcreatetime,flastupdateuserid,flastupdatetime)
		VALUES(#{id},#{code},#{name},#{address},#{contacts},#{mobile},#{qq},#{wx},
		#{email},#{status},#{userId},#{createTime},#{lastupdateUserId},#{lastupdateTime})
	</insert>

	<update id="updateItemSupplier" parameterType="com.clothing.model.item.ItemSupplier">
		update t_clothing_itemsupplier
		<set>
			<if test="cu!=null and cu!=''">fid=#{cu},</if>
			<if test="code!=null and code!=''">fcode=#{code},</if>
			<if test="name!=null and name!=''">fname=#{name},</if>
			<if test="address!=null and address!=''">faddress=#{address},</if>
			<if test="contacts!=null and contacts!=''">fcontacts=#{contacts},</if>
			<if test="mobile!=null and mobile!=''">fmobile=#{mobile},</if>
			<if test="qq!=null and qq!=''">fqq=#{qq},</if>
			<if test="wx!=null and wx!=''">fwx=#{wx},</if>
			<if test="email!=null and email!=''">femail=#{email},</if>
			<if test="status!=null and status!=''">fstatus=#{status},</if>
			<if test="userId!=null and userId!=''">fuserid=#{userId},</if>
			<if test="createTime!=null and createTime!=''">fcreatetime=#{createTime},</if>
			<if test="lastupdateUserId!=null and lastupdateUserId!=''">flastupdateuserid=#{lastupdateUserId},</if>
			<if test="lastupdateTime!=null and lastupdateTime!=''">flastupdatetime=#{lastupdateTime},</if>
		</set>
		where fid=#{id}
	</update>

	<delete id="deleteItemSupplierById" parameterType="string">
		delete from t_clothing_itemsupplier where fid=#{id}
	</delete>


</mapper>