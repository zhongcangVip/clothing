package com.clothing.model.vo;

/**
 * 分页查询VO
 * 
 * @author yangmin
 * @Date:2018年10月31日
 * @param <T>
 */
public class PaginationCondition<T> implements java.io.Serializable{

	private PageInfo pageInfo;

	private T condition;

	public PageInfo getPageInfo() {
		return pageInfo;
	}

	public void setPageInfo(PageInfo pageInfo) {
		this.pageInfo = pageInfo;
	}

	public T getCondition() {
		return condition;
	}

	public void setCondition(T condition) {
		this.condition = condition;
	}

	public PaginationCondition(PageInfo pageInfo, T condition) {
		super();
		this.pageInfo = pageInfo;
		this.condition = condition;
	}

	public PaginationCondition(T condition) {
		super();
		this.condition = condition;
	}

	public PaginationCondition() {
		super();

	}

}
